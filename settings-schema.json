{
	"interval": {
		"type": "spinbutton",
		"default": 1000,
		"min": 750,
		"max": 5000,
		"step": 50,
		"units": "ms",
		"description": "Interval"
	},
	"width": {
		"type": "spinbutton",
		"default": 5,
		"min": 1,
		"max": 20,
		"step": 0.5,
		"units": "px",
		"description": "Line width of graph"
	},
	"byteunit": {
		"type": "combobox",
		"default": true,
		"options": {
			"Bytes with binary prefix": true,
			"Bytes with decimal prefix": false
		},
		"description": "Bytes unit"
	},
	"rateunit": {
		"type": "combobox",
		"default": 1,
		"options": {
			"Bytes with binary prefix per second": 1,
			"Bytes with decimal prefix per second": 0,
			"Bits with binary prefix per second": 3,
			"Bits with decimal prefix per second": 2
		},
		"description": "Rates unit"
	},
	"maxsize": {
		"type": "spinbutton",
		"default": 2000,
		"min": 1000,
		"max": 10000,
		"step": 100,
		"units": " ",
		"description": "Maximal size"
	},
	"order": {
		"type": "combobox",
		"default": true,
		"options": {
			"Write - Read / Up - Down": true,
			"Read - Write / Down - Up": false
		},
		"description": "Order of Disk and Network items"
	},
	"thermalmode": {
		"type": "combobox",
		"default": 1,
		"options": {
			"Minimum": 0,
			"Average": 1,
			"Maximum": 2
		},
		"description": "Thermal calculation mode"
	},
	"colors": {
		"type": "separator"
	},
	"disk": {
		"type": "colorchooser",
		"default": "rgba(73, 168, 53, 1)",
		"description": "Disk"
	},
	"write": {
		"type": "colorchooser",
		"default": "rgba(238, 29, 0, 1)",
		"description": "Write / Up"
	},
	"read": {
		"type": "colorchooser",
		"default": "rgba(45, 125, 179, 1)",
		"description": "Read / Down"
	},
	"cpu1": {
		"type": "colorchooser",
		"default": "rgba(255, 110, 0, 1)",
		"description": "CPU1"
	},
	"cpu2": {
		"type": "colorchooser",
		"default": "rgba(203, 12, 41, 1)",
		"description": "CPU2"
	},
	"cpu3": {
		"type": "colorchooser",
		"default": "rgba(73, 168, 53, 1)",
		"description": "CPU3"
	},
	"cpu4": {
		"type": "colorchooser",
		"default": "rgba(45, 125, 179, 1)",
		"description": "CPU4"
	},
	"mem": {
		"type": "colorchooser",
		"default": "rgba(171, 24, 82, 1)",
		"description": "Memory"
	},
	"swap": {
		"type": "colorchooser",
		"default": "rgba(73, 168, 53, 1)",
		"description": "Swap"
	},
	"notifications": {
		"type": "separator"
	},
	"notificationinfo": {
		"type": "header",
		"description": "The unit int equals one interval"
	},
	"cpuwarning": {
		"type": "checkbox",
		"default": false,
		"description": "enable CPU warning"
	},
	"cpuwarningtime": {
		"type": "spinbutton",
		"min": 1,
		"max": 40,
		"default": 5,
		"step": 1,
		"units": "int",
		"description": "Warning time",
		"indent": true,
		"dependency": "cpuwarning"
	},
	"cpuwarningmode": {
		"type": "checkbox",
		"default": true,
		"description": "Use average CPU usage",
		"indent": true,
		"dependency": "cpuwarning"
	},
	"cpuwarningvalue": {
		"type": "spinbutton",
		"min": 50,
		"max": 100,
		"default": 100,
		"step": 0.5,
		"units": "%",
		"description": "Warning CPU usage",
		"indent": true,
		"dependency": "cpuwarning"
	},
	"thermalwarning": {
		"type": "checkbox",
		"default": false,
		"description": "enable thermal warning"
	},
	"thermalwarningtime": {
		"type": "spinbutton",
		"min": 1,
		"max": 40,
		"default": 5,
		"step": 1,
		"units": "int",
		"description": "Warning time",
		"indent": true,
		"dependency": "thermalwarning"
	},
	"thermalwarningvalue": {
		"type": "spinbutton",
		"min": 25,
		"max": 100,
		"default": 50,
		"step": 0.5,
		"units": "\u00b0C",
		"description": "Warning temperatur",
		"indent": true,
		"dependency": "thermalwarning"
	}
}
