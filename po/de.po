#. settings->disk-panel-label->tooltip
msgid ""
"Common placeholders: \n"
" %write \t write usage \n"
" %read \t read usage"
msgstr ""
"Platzhalter: \n"
" %write \t Schreiben \n"
" %read \t Lesen"

#. settings->thermal-panel-label->tooltip
msgid ""
"Common placeholders: \n"
" %value \t thermal value (specified in \"Thermal Mode\") \n"
" %sensor(digit) \t sensor temperature"
msgstr ""
"Platzhalter: \n"
" %value \t Temperatur (wie in \"Temperaturmodus\") \n"
" %sensor(Zahl) \t Temperatur eines Sensors"

#. settings->cpu-panel-label->tooltip
msgid ""
"Common placeholders: \n"
" %core(digit) \t usage of a core \n"
" %avg \t average usage"
msgstr ""
"Platzhalter: \n"
" %core(Zahl) \t Auslastung eines CPU-Kerns\n"
" %avg \t durchschnittliche Auslastung"

#. settings->network-panel-label->tooltip
msgid ""
"Common placeholders: \n"
" %up \t up usage \n"
" %down \t down usage"
msgstr ""
"Platzhalter: \n"
" %up \t Senden \n"
" %down \t Empfangen"

#. settings->mem-panel-label->tooltip
msgid ""
"Common placeholders: \n"
" %mem.usedup#size \t memory usage in bytes \n"
" %mem.usedup \t memory usage in percent \n"
" %swap#size \t swap usage in bytes \n"
" %swap \t swap usage in bytes"
msgstr ""
"Platzhalter: \n"
" %mem.usedup#size \t Arbeitsspeicherauslastung in Bytes \n"
" %mem.usedup \t Arbeitsspeicherauslastung in Prozent \n"
" %swap#size \t Swap-Auslastung in Bytes \n"
" %swap \t Swap-Auslastung in Prozent"

#. settings->loadAvg-panel-label->tooltip
msgid ""
"Common placeholders: \n"
" %load0 \t load average for the last minute \n"
" %load1 \t load average for the last 5 minutes \n"
" %load2 \t load average for the last 15 minutes"
msgstr ""
"Platzhalter: \n"
" %load0 \t Durchschnittslast der letzten Minute \n"
" %load1 \t Durchschnittslast der letzten 5 Minuten \n"
" %load2 \t Durchschnittslast der letzten 15 Minuten"

